package com.centris.VO;

public class MiscellaneousExpensesVo {

	
	private int sno;
	private String date;
	private String description;
	private double amount;
	private String miscellaneous_expensecode;
	
	public int getSno() {
		return sno;
	}
	public void setSno(int sno) {
		this.sno = sno;
	}
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	public String getMiscellaneous_expensecode() {
		return miscellaneous_expensecode;
	}
	public void setMiscellaneous_expensecode(String miscellaneous_expensecode) {
		this.miscellaneous_expensecode = miscellaneous_expensecode;
	}
	
	
	
	
	
	
}
