	<subDataset name="Local Conveyance">
		<parameter name="loc_empid" class="java.lang.String"/>
		<parameter name="loc_year" class="java.lang.String"/>
		<parameter name="loc_month" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[select la.LC_DATE,la.Location,la.LC_From,la.LC_To,la.Remarks,la.LC_MODE,la.AMOUNT
from hrms_monthly_local_convenyance la
join hrms_monthly_expense me on me.MEXP_CODE=la.MEXP_CODE and me.EMPID = la.EMPID
where la.EMPID=$P{loc_empid} and me.year=$P{loc_year} and me.month=$P{loc_month}]]>
		</queryString>
		<field name="LC_DATE" class="java.sql.Date"/>
		<field name="Location" class="java.lang.String"/>
		<field name="LC_From" class="java.lang.String"/>
		<field name="LC_To" class="java.lang.String"/>
		<field name="Remarks" class="java.lang.String"/>
		<field name="LC_MODE" class="java.lang.String"/>
		<field name="AMOUNT" class="java.lang.Double"/>
		<variable name="loc_total" class="java.lang.Number" calculation="Sum">
			<variableExpression><![CDATA[$F{AMOUNT}]]></variableExpression>
		</variable>
	</subDataset>
