	<parameter name="month" class="java.lang.String"/>
	<parameter name="year" class="java.lang.String"/>
	<parameter name="managerCode" class="java.lang.String"/>
	<parameter name="empCode" class="java.lang.String"/>
	<parameter name="monthn" class="java.lang.String"/>
	<parameter name="LOGO_DIR" class="java.lang.String"/>
	<queryString>
		<![CDATA[select emp.EMPID,emp.bioid,emp.First_Name,des.designationName,emp.Mobile_Number,emp.Official_Email_ID,hrm.FirstName as ManagerName,
case when expe.Local_conveyance is null then '0' else expe.Local_conveyance end Local_conveyance,
case when expe.Travel_expenses is null then '0' else expe.Travel_expenses end Travel_expenses,
case when expe.Staff_welfare is null then '0' else expe.Staff_welfare end Staff_welfare,
case when expe.Telephone_expenses is null then '0' else expe.Telephone_expenses end Telephone_expenses,
case when expe.Purchases is null then '0' else expe.Purchases end Purchases,
case when expe.other_work_expenses is null then '0' else expe.other_work_expenses end other_work_expenses,
case when expe.Miscellaneous is null then '0' else expe.Miscellaneous end Miscellaneous,
case when expe.Total_amount_claimed is null then '0' else expe.Total_amount_claimed end Total_amount_claimed,
case when expe.Total_amount_approved is null then '0' else expe.Total_amount_approved end Total_amount_approved,
case when expe.Advance_paid is null then '0' else expe.Advance_paid end Total_advance_paid,
case when expe.Balance is null then '0' else expe.Balance end Total_balance
from hrms_employee emp
left join hrms_monthly_expense expe on expe.EMPID=emp.EMPID
join hrms_designation des on des.DesignationCode=emp.DesignationCode
join hrms_manager hrm on hrm.ManagerCode=expe.ManagerCode
where expe.year=$P{year} and expe.month=$P{month} and expe.ManagerCode=$P{managerCode}  and emp.EMPID=$P{empCode}]]>
	</queryString>
	<field name="EMPID" class="java.lang.String"/>
	<field name="bioid" class="java.lang.Integer"/>
	<field name="First_Name" class="java.lang.String"/>
	<field name="designationName" class="java.lang.String"/>
	<field name="Mobile_Number" class="java.lang.String"/>
	<field name="Official_Email_ID" class="java.lang.String"/>
	<field name="ManagerName" class="java.lang.String"/>
	<field name="Local_conveyance" class="java.lang.String"/>
	<field name="Travel_expenses" class="java.lang.String"/>
	<field name="Staff_welfare" class="java.lang.String"/>
	<field name="Telephone_expenses" class="java.lang.String"/>
	<field name="Purchases" class="java.lang.String"/>
	<field name="other_work_expenses" class="java.lang.String"/>
	<field name="Miscellaneous" class="java.lang.String"/>
	<field name="Total_amount_claimed" class="java.lang.String"/>
	<field name="Total_amount_approved" class="java.lang.String"/>
	<field name="Total_advance_paid" class="java.lang.String"/>
	<field name="Total_balance" class="java.lang.String"/>
	<variable name="slno" class="java.lang.Integer" calculation="Count">
		<variableExpression><![CDATA[$F{EMPID}]]></variableExpression>
	</variable>
